<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SpecialistCatalog.Views.UP_Labs.UPLab3Detail"
             Title="Определение операций и формирование сметы">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout Padding="10">
                <Label Text="Матрица ответственности" FontSize="Medium" VerticalOptions="Center" HorizontalOptions="Center"/>
                <Button Text="Открыть в браузере" Clicked="Button_Clicked"/>
            </StackLayout>

            <StackLayout Padding="10">
                <Label Text="Определение длительности процессов" FontSize="Medium" VerticalOptions="Center" HorizontalOptions="Center"/>
                <Button Text="Открыть в браузере" Clicked="Button_Clicked_1"/>
            </StackLayout>

            <StackLayout Padding="10">
                <Label Text="Смета" FontSize="Medium" VerticalOptions="Center" HorizontalOptions="Center"/>
                <Button Text="Открыть в браузере" Clicked="Button_Clicked_2"/>
            </StackLayout>

            <StackLayout Padding="10">
                <Label Text="Содержание проекта" FontSize="Medium" VerticalOptions="Center" HorizontalOptions="Center"/>
                <Button Text="Открыть в браузере" Clicked="Button_Clicked_3"/>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>